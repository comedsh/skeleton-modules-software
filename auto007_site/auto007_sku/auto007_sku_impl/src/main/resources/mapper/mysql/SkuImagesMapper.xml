<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fenghua.auto.sku.domain.SkuImages" >
  <resultMap id="BaseResultMap" type="com.fenghua.auto.sku.domain.SkuImages" >
    <id column="id" property="id" jdbcType="BIGINT" />
    <result column="title" property="title" jdbcType="CHAR" />
    <result column="sort_no" property="sortNo" jdbcType="INTEGER" />
    <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
    <result column="update_time" property="updateTime" jdbcType="TIMESTAMP" />
    <result column="sku_id" property="skuId" jdbcType="BIGINT" />
     <result column="url" property="url" jdbcType="LONGVARCHAR" />
  </resultMap>
  <sql id="Base_Column_List" >
    id, title, sort_no, create_time, update_time, sku_id, url
  </sql>
  
  
  <select id="select" resultMap="BaseResultMap" parameterType="com.fenghua.auto.sku.domain.SkuImages" >
    select 
    <include refid="Base_Column_List" />
    from sku_images
    where 1=1
    <if test="skuId != null" >
        and sku_id=#{skuId,jdbcType=BIGINT}
    </if>
    <if test="id != null" >
       and id = #{id,jdbcType=BIGINT}
    </if>
    order by sort_no
  </select>
 

  
</mapper>